{
  "name": "SaneSwift",
  "platforms": {
    "ios": "8.0"
  },
  "version": "1.0.0",
  "license": "Custom",
  "summary": "Swift wrapper for SANE backends",
  "homepage": "https://github.com/dvkch/SaneScanner",
  "authors": {
    "Stan Chevallier": "contact@stanislaschevallier.fr"
  },
  "source": {
    "git": "https://github.com/dvkch/SaneScanner.git",
    "tag": "1.0.0"
  },
  "swift_versions": "4.2",
  "source_files": [
    "SaneSwift/*.{h,c,m,swift}",
    "sane-libs/all/*.h"
  ],
  "vendored_libraries": "sane-libs/all/**/libsane-net.a",
  "resource_bundles": {
    "libSANE-Translations": [
      "sane-libs/all/translations/*.lproj"
    ],
    "SaneSwift-Translations": [
      "SaneSwift/Localizable/*.lproj"
    ]
  },
  "requires_arc": true,
  "xcconfig": {
    "CLANG_MODULES_AUTOLINK": "YES"
  },
  "swift_version": "4.2"
}
